{
  "builds": [
    {
      "src": "user-tracking/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles_build" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "user-tracking/wsgi.py"
    }
  ],
  "functions": {
    "user-tracking/wsgi.py": {
      "runtime": "python3.10",
      "maxDuration": 30
    }
  }
}
